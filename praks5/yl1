#! /bin/bash

read -p "Sisesta vanus: " vanus

if  test $vanus -gt 0 -a $vanus -lt 11; then
	echo "Sa oled laps."
elif test $vanus -ge 11 -a $vanus -lt 18; then
	echo "Sa oled nooruk."
elif test $vanus -ge 18 -a $vanus -lt 63; then
	echo "Sa oled tÃ¤iskasvanu."
elif test $vanus -ge 63; then
	echo "Sa oled senioor."
else
	echo "Vigane vanus"
fi
